dconf write /org/gnome/desktop/wm/keybindings/maximize "['<Primary><Super>Up']"
dconf write /org/gnome/desktop/wm/keybindings/unmaximize "['<Primary><Super>Down', '<Alt>F5']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-up "['<Primary><Shift><Alt>Up', '<Control><Shift><Alt>Up']"
dconf write /org/gnome/desktop/wm/keybindings/toggle-fullscreen "['F11']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-right "['<Primary><Shift><Alt>Right']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-left "['<Primary><Shift><Alt>Left']"
dconf write /org/gnome/desktop/wm/keybindings/switch-windows "['<Alt>Tab']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-down "['<Primary><Shift><Alt>Down', '<Control><Shift><Alt>Down']"

dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom2/binding "'<Primary><Alt>v'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom2/command "'vlc'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom2/name "'VLC'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom1/binding "'<Primary><Alt>t'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom1/command "'gnome-terminal'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom1/name "'Terminal'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0/binding "'<Primary><Alt>f'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0/command "'firefox'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0/name "'Firefox'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom3/binding "'<Primary><Alt>e'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom3/command "'nautilus'"
dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom3/name "'Nautilus'"

